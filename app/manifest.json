{
  "name": "Share highlighted text",
  "version": "0.0.1",
  "manifest_version": 2,
  "default_locale": "ja",
  "background": {
    "scripts": [
      "libs/polyfill.min.js",
      "scripts.babel/background.js"
    ]
  },
  "permissions": [
    "background",
    "activeTab",
    "contextMenus",
    "clipboardWrite"
  ]
}